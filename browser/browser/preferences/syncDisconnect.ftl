sync-disconnect-dialog =
    .title = Afbryd forbindelse til { -sync-brand-short-name }?
    .style = width: 36em; min-height: 35em;

sync-disconnect-heading = Vil du samtidig fjerne browser-data fra denne computer? Dine { -sync-brand-name }-data vil forblive på din konto.

sync-disconnect-remove-sync-caption = Fjern { -sync-brand-name }-data

sync-disconnect-remove-sync-data = Bogmærker, historik, adgangskoder osv.

sync-disconnect-remove-other-caption = Fjern andre private data

sync-disconnect-remove-other-data = Cookies, cache, offline websteds-data osv.

# Shown while the disconnect is in progress
sync-disconnect-disconnecting = Afbryder…

sync-disconnect-cancel =
    .label = Annuller
    .accesskey = A

## Disconnect confirm Button
##
## The 2 labels which may be shown on the single "Disconnect" button, depending
## on the state of the checkboxes.
sync-disconnect-confirm-disconnect-delete =
    .label = Afbryd og slet
    .accesskey = s

sync-disconnect-confirm-disconnect =
    .label = Afbryd kun
    .accesskey = k
