blocklist-window =
    .title = ပိတ်ပင်ထားသည့် စာရင်းများ
    .style = width: 55em
blocklist-desc = သင့်ရှာဖွေမှုကို ခြေရာခံကောင်းခံနိုင်သည့် ဝဘ်အဲလိမန့်ကို မီးမြေခွေးက ပိတ်ရမည့်စာရင်းကို သင်ရွေးချယ်နိုင်ပါသည်။
blocklist-close-key =
    .key = w
blocklist-treehead-list =
    .label = စာရင်း
blocklist-button-cancel =
    .label = မလုပ်ဆောင်တော့ပါ
    .accesskey = C
blocklist-button-ok =
    .label = ပြောင်းလဲမှုများကို သိမ်းပါ
    .accesskey = S
