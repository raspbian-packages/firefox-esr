if test -n "$MOZ_TSAN" -o -n "$MOZ_ASAN"; then
    # Bug 1182565: TSan conflicts with sandboxing on Linux.
    # Bug 1287971: LSan also conflicts with sandboxing on Linux.
    case $OS_TARGET in
    Linux|Android)
        MOZ_SANDBOX=
        ;;
    esac
fi

# Check whether --enable-sandbox or --disable-sandbox was given.
if test "${enable_sandbox+set}" = set; then
  enableval="$enable_sandbox"
  if test "$enableval" = "no"; then
    MOZ_SANDBOX=
  elif test "$enableval" = "yes"; then
    MOZ_SANDBOX=1
  else
    { echo "configure: error: Option, sandbox, does not take an argument ($enableval)." 1>&2; echo "configure: error: Option, sandbox, does not take an argument ($enableval)." 1>&5; exit 1; }
  fi
fi



case "$OS_TARGET" in
WINNT)
    MOZ_CONTENT_SANDBOX=$MOZ_SANDBOX
    ;;
Darwin)
    MOZ_CONTENT_SANDBOX=$MOZ_SANDBOX
    ;;
Linux)
    case $CPU_ARCH in
        x86_64|x86)
            MOZ_CONTENT_SANDBOX=$MOZ_SANDBOX
        ;;
    esac
    ;;
esac

# Check whether --enable-content-sandbox or --disable-content-sandbox was given.
if test "${enable_content_sandbox+set}" = set; then
  enableval="$enable_content_sandbox"
  if test "$enableval" = "yes"; then
    MOZ_CONTENT_SANDBOX=1
  elif test "$enableval" = "no"; then
    MOZ_CONTENT_SANDBOX=
  else
    { echo "configure: error: Option, content-sandbox, does not take an argument ($enableval)." 1>&2; echo "configure: error: Option, content-sandbox, does not take an argument ($enableval)." 1>&5; exit 1; }
  fi
fi


if test -n "$MOZ_CONTENT_SANDBOX" -a -z "$MOZ_SANDBOX"; then
    { echo "configure: error: --enable-content-sandbox and --disable-sandbox are conflicting options" 1>&2; echo "configure: error: --enable-content-sandbox and --disable-sandbox are conflicting options" 1>&5; exit 1; }
fi

if test -n "$MOZ_CONTENT_SANDBOX"; then
    cat >> confdefs.pytmp <<\EOF
    (''' MOZ_CONTENT_SANDBOX ''', ' 1 ')
EOF
cat >> confdefs.h <<\EOF
#define MOZ_CONTENT_SANDBOX 1
EOF

fi



case $OS_TARGET in
WINNT)
    MOZ_GMP_SANDBOX=$MOZ_SANDBOX
    ;;
Linux)
    case $CPU_ARCH in
    x86_64|x86)
        MOZ_GMP_SANDBOX=$MOZ_SANDBOX
        ;;
    esac
    ;;
Darwin)
    MOZ_GMP_SANDBOX=$MOZ_SANDBOX
    ;;
esac

if test -n "$MOZ_GMP_SANDBOX"; then
    cat >> confdefs.pytmp <<\EOF
    (''' MOZ_GMP_SANDBOX ''', ' 1 ')
EOF
cat >> confdefs.h <<\EOF
#define MOZ_GMP_SANDBOX 1
EOF

fi



if test -z "$MOZ_CONTENT_SANDBOX" -a -z "$MOZ_GMP_SANDBOX"; then
    MOZ_SANDBOX=
fi

if test -n "$MOZ_SANDBOX"; then
    cat >> confdefs.pytmp <<\EOF
    (''' MOZ_SANDBOX ''', ' 1 ')
EOF
cat >> confdefs.h <<\EOF
#define MOZ_SANDBOX 1
EOF

fi




