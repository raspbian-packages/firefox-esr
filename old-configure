fi
rm -f conftest*
    CFLAGS=$_SAVE_CFLAGS

    case "${CPU_ARCH}" in
    x86 | x86_64)
      echo $ac_n "checking for x86 AVX2 asm support in compiler""... $ac_c" 1>&6
echo "configure:5234: checking for x86 AVX2 asm support in compiler" >&5
      cat > conftest.$ac_ext <<EOF
#line 5236 "configure"
#include "confdefs.h"

int main() {
asm volatile ("vpermq      \$0xd8,%ymm0,%ymm0 \n");
; return 0; }
EOF
if { (eval echo configure:5243: \"$ac_compile\") 1>&5; (eval $ac_compile) 2>&5; }; then
  rm -rf conftest*
  result="yes"
else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
  rm -rf conftest*
  result="no"
fi
rm -f conftest*
      echo "$ac_t"""$result"" 1>&6
      if test "$result" = "yes"; then
          HAVE_X86_AVX2=1
      fi
      ;;

    ppc*)
