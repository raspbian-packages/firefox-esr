    echo $ac_n "checking for png_get_acTL in -lpng""... $ac_c" 1>&6
echo "configure:9995: checking for png_get_acTL in -lpng" >&5
ac_lib_var=`echo png'_'png_get_acTL | sed 'y%./+-%__p_%'`
