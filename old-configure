        MOZ_ENABLE_DWRITE_FONT=1

        if test "$COMPILE_ENVIRONMENT"; then

             ac_safe=`echo "d3d9.h" | sed 'y%./+-%__p_%'`
  echo $ac_n "checking for d3d9.h""... $ac_c" 1>&6
