EOF

cat >> confdefs.pytmp <<EOF
    (''' MOZ_B2G_OS_NAME ''', r''' "$MOZ_B2G_OS_NAME" ''')
EOF
cat >> confdefs.h <<EOF
#define MOZ_B2G_OS_NAME "$MOZ_B2G_OS_NAME"
EOF















