                echo "$ac_t""yes" 1>&6
else
  rm -rf conftest*
  echo "$ac_t""no" 1>&6
fi
rm -f conftest*


echo $ac_n "checking for _aligned_malloc in malloc.h""... $ac_c" 1>&6
echo "configure:7900: checking for _aligned_malloc in malloc.h" >&5
cat > conftest.$ac_ext <<EOF
#line 7902 "configure"
#include "confdefs.h"
#include <malloc.h>
EOF
if (eval "$ac_cpp conftest.$ac_ext") 2>&5 |
  egrep "_aligned_malloc" >/dev/null 2>&1; then
  rm -rf conftest*
  cat >> confdefs.pytmp <<\EOF
    (''' HAVE_ALIGNED_MALLOC ''', ' 1 ')
EOF
cat >> confdefs.h <<\EOF
#define HAVE_ALIGNED_MALLOC 1
EOF

                echo "$ac_t""yes" 1>&6
else
  rm -rf conftest*
  echo "$ac_t""no" 1>&6
fi
rm -f conftest*




fi

