        fi
        # We use MOZ_PATH_PROG in order to get a Windows style path.
         # Extract the first word of "$MOZ_CLANG_RT_ASAN_LIB", so it can be a program name with args.
set dummy $MOZ_CLANG_RT_ASAN_LIB; ac_word=$2
