fi
rm -f conftest*
    cat > conftest.$ac_ext <<EOF
#line 10324 "configure"
#include "confdefs.h"
#include "prtypes.h"
int main() {
#ifndef PR_UINT64
                 #error PR_UINT64 not defined or requires including prtypes.h
                 #endif
; return 0; }
