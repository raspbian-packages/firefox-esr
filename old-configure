#line 4990 "configure"
#include "confdefs.h"

int main() {

; return 0; }
EOF
if { (eval echo configure:4997: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
  :
else
