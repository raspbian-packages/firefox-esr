EOF

  ;;

xulrunner)
  cat >> confdefs.pytmp <<\EOF
    (''' MOZ_XULRUNNER ''', ' 1 ')
EOF
cat >> confdefs.h <<\EOF
#define MOZ_XULRUNNER 1
EOF

  ;;
esac

