    for version in 23.0.3 23.0.1; do
        android_build_tools="$android_build_tools_base"/$version
        if test -d "$android_build_tools" -a -f "$android_build_tools/aapt"; then
            android_build_tools_version=$version
            echo "$ac_t""$android_build_tools" 1>&6
            break
        fi
    done
    if test "$android_build_tools_version" = ""; then
        version=$(echo 23.0.3 23.0.1 | cut -d" " -f1)
        { echo "configure: error: You must install the Android build-tools version $version.  Try |mach bootstrap|.  (Looked for "$android_build_tools_base"/$version)" 1>&2; echo "configure: error: You must install the Android build-tools version $version.  Try |mach bootstrap|.  (Looked for "$android_build_tools_base"/$version)" 1>&5; exit 1; }
