              return !sqlite3_compileoption_used("SQLITE_SECURE_DELETE");
            }
EOF
if { (eval echo configure:13084: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext} && (./conftest; exit) 2>/dev/null
