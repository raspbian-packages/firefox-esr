  echo "$ac_t""$OTOOL" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

test -n "$OTOOL" && break
done
test -n "$OTOOL" || OTOOL=":"

    if test -z "$HOST_RANLIB"; then
        HOST_RANLIB="$RANLIB"
    fi
    if test -z "$HOST_AR"; then
        HOST_AR="$AR"
    fi
