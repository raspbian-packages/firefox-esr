        { echo "configure: error: Including platforms/android-* in --with-android-sdk arguments is deprecated.  Use --with-android-sdk=$android_sdk_root." 1>&2; echo "configure: error: Including platforms/android-* in --with-android-sdk arguments is deprecated.  Use --with-android-sdk=$android_sdk_root." 1>&5; exit 1; }
    fi

    android_target_sdk=23
