  ac_cv_sqlite_secure_delete=no
        
else
  cat > conftest.$ac_ext <<EOF
#line 15986 "configure"
#include "confdefs.h"

            #include "sqlite3.h"

            int main(int argc, char **argv){
              return !sqlite3_compileoption_used("SQLITE_SECURE_DELETE");
            }
EOF
if { (eval echo configure:15995: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext} && (./conftest; exit) 2>/dev/null
