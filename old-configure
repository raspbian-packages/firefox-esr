    if test "$COMPILE_ENVIRONMENT"; then

            if test -n "$WIN32_D2D_SURFACE_FEATURE"; then
           ac_safe=`echo "d3d10.h" | sed 'y%./+-%__p_%'`
  echo $ac_n "checking for d3d10.h""... $ac_c" 1>&6
