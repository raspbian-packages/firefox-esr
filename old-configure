  fi
fi


if test -n "$_USE_SYSTEM_NSS"; then
    
# Check whether --with-nss-prefix or --without-nss-prefix was given.
if test "${with_nss_prefix+set}" = set; then
  withval="$with_nss_prefix"
  nss_config_prefix="$withval"
else
