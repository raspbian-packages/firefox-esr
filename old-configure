  elif test "$enableval" = "no"; then
    MOZ_ENABLE_LIBPROXY=
  else
    { echo "configure: error: Option, libproxy, does not take an argument ($enableval)." 1>&2; echo "configure: error: Option, libproxy, does not take an argument ($enableval)." 1>&5; exit 1; }
